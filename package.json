{
  "name": "babel-plugin-runtyper",
  "version": "0.1.3",
  "description": "Runtime type checker for JavaScript code without annotations",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/babel-plugin-runtyper.git"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "src/index.js",
  "scripts": {
    "code": "npm run lint-all",
    "lint-all": "eslint src test examples",
    "lint-staged": "lint-staged",
    "test": "mocha test/specs --require test/setup",
    "ci": "npm run code && npm test",
    "release-patch": "npm run code && npm test && npm version patch && npm publish && git push --follow-tags --no-verify",
    "example-browser": "cd examples/browser && ../../node_modules/.bin/webpack",
    "example-node": "cd examples/node && ../../node_modules/.bin/babel ./app.js -o ./app.instrumented.js --plugins=babel-plugin-runtyper --source-maps",
    "examples": "npm run example-browser && npm run example-node",
    "precommit": "npm run lint-staged",
    "prepush": "npm run code && npm test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "babel-template": "^6.23.0",
    "babel-types": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "mocha": "^3.2.0",
    "source-map-support": "^0.4.14",
    "webpack": "^2.3.2"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "keywords": [
    "type-checking",
    "type-hints",
    "type-safety",
    "runtime-validation",
    "babel-plugin"
  ],
  "license": "MIT"
}
