{
  "name": "babel-plugin-runtyper",
  "version": "0.1.1",
  "description": "Runtime type checker for JavaScript",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/babel-plugin-runtyper.git"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "src/index.js",
  "scripts": {
    "code": "npm run lint-all",
    "lint-all": "eslint src test",
    "lint-staged": "lint-staged",
    "test": "npm run babel && npm run mocha",
    "babel": "mkdir -p test/instrumented && babel test/samples -o test/instrumented/index.js --source-maps --plugins=../../src/index.js",
    "mocha": "mocha test/specs --require test/setup",
    "ci": "npm run code && npm test",
    "release-patch": "npm run code && npm test && npm version patch && npm publish && git push --follow-tags --no-verify",
    "precommit": "npm run lint-staged",
    "prepush": "npm run code && npm test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "babel-template": "^6.23.0",
    "babel-types": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "mocha": "^3.2.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "keywords": [
    "type-checking",
    "runtime",
    "babel",
    "babel-plugin"
  ],
  "license": "MIT"
}
